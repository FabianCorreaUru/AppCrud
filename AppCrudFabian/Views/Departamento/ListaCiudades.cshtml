@{
}

<div class="card">
    <div class="card-header">
        <h3 class="text-center"> Ciudades del Departamento De @ViewData["DepartamentoNombre"] </h3>
    </div>
    <div class="card-body">
        <div class="row">
             @foreach (var item in @ViewData["DepartamentoCiudades"] as List<Ciudad>)
             {
                 <div class="col-3 text-center">
                      <h5 class="text-success mt-3">@item.Nombre</h5>
                 </div>
             }
        </div>
     </div>
</div>